{
  "name": "angular-loaders-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/angular-loaders-lib/src",
  "projectType": "library",
  "prefix": "ngx",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "options": {
        "tsConfig": "libs/angular-loaders-lib/tsconfig.lib.json",
        "project": "libs/angular-loaders-lib/ng-package.json"
      },
      "configurations": {
        "production": {
          "project": "libs/angular-loaders-lib/ng-package.prod.json"
        }
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "libs/angular-loaders-lib/src/test.ts",
        "tsConfig": "libs/angular-loaders-lib/tsconfig.spec.json",
        "karmaConfig": "libs/angular-loaders-lib/karma.conf.js"
      }
    }
  }
}
